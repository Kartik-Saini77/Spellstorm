config_version=5

[application]

config/name="Spellstorm"
run/main_scene="res://scenes/menu.tscn"
config/quit_on_go_back=false
config/features=PackedStringArray("4.3", "GL Compatibility")
boot_splash/image="res://assets/Spellstorm-icons/Loading.png"
boot_splash/fullsize=false
config/icon="res://icon.svg"
config/windows_native_icon="res://icon.ico"

[display]

window/size/viewport_width=960
window/size/viewport_height=540
window/stretch/mode="viewport"
window/handheld/orientation=4

[global_group]

Player="refers to the player sprite"
WebProjectile="refers to web_projectile"
Fireball="refers to the fireball player attack"
Spider="refers to the spider enemy"

[gui]

theme/default_font_multichannel_signed_distance_field=true
